{
  "kind": "collectionType",
  "collectionName": "landingpages",
  "info": {
    "name": "landingpage",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "slug": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "description": {
      "type": "string"
    },
    "seo_title": {
      "type": "string"
    },
    "seo_description": {
      "type": "string"
    },
    "seo_keyword": {
      "type": "string"
    },
    "social_title": {
      "type": "string"
    },
    "social_description": {
      "type": "string"
    },
    "social_image": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "images",
        "files",
        "videos"
      ],
      "plugin": "upload",
      "required": false
    },
    "content": {
      "type": "richtext"
    },
    "slider": {
      "type": "component",
      "repeatable": true,
      "component": "home.feature"
    },
    "section": {
      "type": "component",
      "repeatable": true,
      "component": "home.block"
    },
    "social": {
      "type": "component",
      "repeatable": true,
      "component": "home.social"
    },
    "notification": {
      "type": "dynamiczone",
      "components": [
        "home.notification"
      ]
    },
    "footermenu": {
      "type": "component",
      "repeatable": true,
      "component": "home.menusection"
    }
  }
}
